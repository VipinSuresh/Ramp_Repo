/* convert from unix timestamp to date */
with date_conv as
(
 	SELECT 
		module_id, 
		to_timestamp(created_date) AS readable_date
	FROM modules
)
/* use the above readable date to filter */
SELECT 
	module_id
FROM date_conv
	WHERE readable_date >= '2023-04-01T00:00:00.000Z' 
    AND readable_date < '2023-05-01T00:00:00.000Z'
	ORDER BY readable_date
	LIMIT 1;
